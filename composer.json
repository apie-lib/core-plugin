{
  "name": "apie/core-plugin",
  "description": "Composer package of the apie library: core plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\CorePlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\CorePlugin\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/core": "dev-main",
    "laminas/laminas-diactoros": "^2.1",
    "pagerfanta/pagerfanta": "^2.4",
    "psr/http-message": "^1.0",
    "symfony/property-info": "^4.3|^5.0",
    "symfony/serializer": "^4.3|^5.0"
  },
  "require-dev": {
    "apie/mock-objects": "dev-main",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}